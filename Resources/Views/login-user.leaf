#set("title") { Log in }

#set("body") {
    <h1>Log in</h1>
    <form method="post">
        <p>Username: <input type="text" name="username" /></p>
        <p>Password: <input type="password" name="password" /></p>
        #if(error) {
            <p>* #(error)</p>
        }
        <p>
            <input type="submit" class="btn btn-lg btn-primary" />
        </p>
    </form>
}

#embed("master")
